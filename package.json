{
	"name": "js-cool",
	"description": "这是一个纯原生ES6开发的Javascript常用方法库",
	"version": "3.0.0-beta.6",
	"packageManager": "pnpm@7.30.0",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"browser": {
		"dist/index.js": "dist/index.min.js"
	},
	"unpkg": "./dist/index.min.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./lib": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./lib/*": {
			"types": "./lib/*.d.ts",
			"require": "./lib/*.cjs",
			"import": "./lib/*.mjs"
		},
		"./*": {
			"types": "./lib/*.d.ts",
			"require": "./lib/*.cjs",
			"import": "./lib/*.mjs"
		}
	},
	"directories": {
		"lib": "lib",
		"dist": "dist",
		"src": "src"
	},
	"files": [
		"lib",
		"dist",
		"typings"
	],
	"scripts": {
		"gen:version": "tscjs scripts/version",
		"tag:version": "tscjs scripts/tag",
		"build": "run-s clean build:{bundle,es5,types,docs} roll-types",
		"build:umd": "cross-env BABEL_ENV=es5 webpack --config webpack.config.js",
		"build:bundle": "rollup -c build/rollup.config.ts --configPlugin @rollup/plugin-typescript",
		"build:es5": "cross-env BABEL_ENV=es5 pnpm build:bundle",
		"build:types": "tsc -p src/tsconfig.declaration.json",
		"build:types1": "tsc --emitDeclarationOnly --outDir lib -p src",
		"build:docs": "rm-all docs && typedoc && prettier --write \"**/*.md\"",
		"roll-types": "api-extractor run",
		"watch": "pnpm build:bundle -w",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"workflow:publish-test": "zx scripts/workflow.mjs",
		"clean": "rm-all dist es lib",
		"dist": "run-s eslint prettier build",
		"test-unit": "jest --filter ./scripts/filter-unit.js",
		"eslint": "eslint --fix .",
		"prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\"",
		"pub.bak": "git add . && git commit -m $npm_package_version && git pull && git push"
	},
	"dependencies": {
		"core-js": "^3.29.1"
	},
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@babel/plugin-transform-runtime": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@eslint-sets/eslint-config-ts": "^5.0.0",
		"@microsoft/api-extractor": "^7.34.4",
		"@rollup/plugin-alias": "^4.0.3",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.1",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-terser": "^0.4.0",
		"@rollup/plugin-typescript": "^11.0.0",
		"@types/babel__core": "^7.20.0",
		"@types/node": "^18.15.6",
		"babel-loader": "^9.1.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.36.0",
		"fast-glob": "^3.2.12",
		"load-yml": "^1.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.6",
		"prettier-config-common": "^1.4.0",
		"reinstaller": "^2.3.0",
		"rm-all": "^1.0.0",
		"rollup": "^3.20.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-filesize": "^10.0.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"tsnd": "^1.1.0",
		"typedoc": "^0.23.28",
		"typedoc-plugin-markdown": "^3.14.0",
		"typescript": "^5.0.2",
		"zx": "^7.2.1"
	},
	"peerDependencies": {
		"core-js": ">=3.0.0"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	},
	"keywords": [
		"js-cool",
		"es6",
		"javascript",
		"js library",
		"js lib",
		"js tools",
		"saqqdy"
	],
	"license": "MIT",
	"author": "saqqdy<https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/js-cool#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/js-cool/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/js-cool.git"
	}
}
