{
	"name": "js-cool",
	"description": "这是一个纯原生ES6开发的Javascript常用方法库",
	"version": "2.5.0",
	"packageManager": "pnpm@7.7.0",
	"main": "lib/index.js",
	"module": "es/index.mjs",
	"browser": {
		"dist/index.js": "dist/index.min.js"
	},
	"unpkg": "lib/index.js",
	"typings": "es/index.d.ts",
	"exports": {
		".": {
			"require": "./lib/index.js",
			"import": "./es/index.mjs",
			"types": "./es/index.d.ts"
		},
		"./es": "./es/index.mjs",
		"./lib": "./lib/index.js",
		"./es/*.mjs": "./es/*.mjs",
		"./es/*": "./es/*.mjs",
		"./lib/*.js": "./lib/*.js",
		"./lib/*": "./lib/*.js",
		"./*": "./*"
	},
	"directories": {
		"es": "es",
		"lib": "lib",
		"dist": "dist",
		"src": "src"
	},
	"files": [
		"es",
		"lib",
		"dist",
		"typings"
	],
	"scripts": {
		"gen:version": "tscjs scripts/version",
		"tag:version": "tscjs scripts/tag",
		"build": "rimraf lib es && rollup -c",
		"build:umd": "webpack --config webpack.config.js",
		"build:types": "run-s build-temp-types roll-types",
		"build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p src/",
		"patch-types": "node scripts/patchTypes",
		"roll-types": "api-extractor run && rimraf temp",
		"dev": "rollup -c -w",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"test": "node test/test.js",
		"pretest": "npm run build",
		"dist": "run-s eslint prettier build build:umd build:types docs prettier:docs",
		"docs": "rimraf docs && typedoc",
		"eslint": "eslint --fix --ext .ts,.js",
		"prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"prettier:docs": "prettier --write \"docs/**/*.{json,md}\"",
		"pub.bak": "git add . && git commit -m $npm_package_version && git pull && git push"
	},
	"dependencies": {
		"core-js": "^3.25.1",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.19.0",
		"@babel/plugin-transform-runtime": "^7.18.10",
		"@babel/preset-env": "^7.19.0",
		"@babel/preset-typescript": "^7.18.6",
		"@eslint-sets/eslint-config-simple-ts": "^3.0.5",
		"@microsoft/api-extractor": "^7.30.0",
		"@rollup/plugin-babel": "^6.0.0",
		"@rollup/plugin-commonjs": "^23.0.0",
		"@rollup/plugin-node-resolve": "^15.0.0",
		"@types/babel__core": "^7.1.19",
		"@types/node": "^18.7.16",
		"babel-loader": "^9.0.0",
		"eslint": "^8.23.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-config-common": "^1.2.1",
		"progress-bar-webpack-plugin": "^2.1.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.79.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.34.0",
		"terser-webpack-plugin": "^5.3.6",
		"tsnd": "^1.1.0",
		"typedoc": "^0.23.14",
		"typedoc-plugin-markdown": "^3.13.6",
		"typescript": "^4.8.3",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-merge": "^5.8.0",
		"webpack-node-externals": "^3.0.0"
	},
	"peerDependencies": {
		"core-js": ">=3.0.0",
		"tslib": ">=2.0.0"
	},
	"keywords": [
		"js-cool",
		"es6",
		"javascript",
		"js library",
		"js lib",
		"js tools",
		"saqqdy"
	],
	"license": "MIT",
	"author": "saqqdy<https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/js-cool.git"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"tslib",
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	},
	"bugs": {
		"url": "https://github.com/saqqdy/js-cool/issues"
	},
	"homepage": "https://github.com/saqqdy/js-cool#readme"
}
